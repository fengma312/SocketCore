<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>

    <Title>SocketCore</Title>
    <Version>1.0.15</Version>
    <AssemblyVersion>1.0.15</AssemblyVersion>
    <FileVersion>1.0.15</FileVersion>
    <PackageProjectUrl>https://github.com/fengma312/SocketCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fengma312/SocketCore</RepositoryUrl>
    <PackageTags>websocket;tcp;udp;SocketCore;</PackageTags>
    <Description>tcp,udp,websocket服务端和客户端框架</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
  </ItemGroup>

</Project>
