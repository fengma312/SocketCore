<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>

    <Title>SocketCore</Title>
    <AssemblyVersion>1.0.15</AssemblyVersion>
    <FileVersion>1.0.15</FileVersion>
    <PackageProjectUrl>https://github.com/fengma312/SocketCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fengma312/SocketCore</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>websocket;tcp;udp;Socket.Core;</PackageTags>
    <Description>tcp,udp,websocket服务端和客户端框架</Description>
  </PropertyGroup>

</Project>
